{
  "name": "places-autocomplete",
  "version": "0.1.4",
  "description": "_custom UI for Google Maps Places Autocomplete",
  "main": "dist/places-autocomplete.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress",
    "prettier": "prettier --write \"src/**/*.js\" \"demo/**/*.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format-code": "npm run prettier & npm run lint:fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rgbutov/places-autocomplete"
  },
  "keywords": [
    "front-end",
    "browser",
    "Googe Maps",
    "google maps",
    "google-maps",
    "@google-maps",
    "google-maps-api",
    "@google-maps-api",
    "MapsAPI",
    "GoogleMap",
    "input dropdawn",
    "npm",
    "package"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Ruslan Butov <rgbutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgbutov/places-autocomplete/issues"
  },
  "homepage": "https://github.com/rgbutov/places-autocomplete#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
